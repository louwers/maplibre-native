t"},{"type":"text","text":"transition. If you don’t want to animate the change, use the"},{"type":"text","text":" "},{"code":"-setTargetCoordinate:animated:","type":"codeVoice"},{"type":"text","text":" method instead."}]}]}],"metadata":{"modules":[{"name":"MapLibre"}],"roleHeading":"Instance Property","title":"targetCoordinate","symbolKind":"property","fragments":[{"kind":"identifier","text":"targetCoordinate"}],"role":"symbol","navigatorTitle":[{"text":"targetCoordinate","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(py)targetCoordinate"},"sections":[],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"]}],"kind":"symbol","identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"type":"text","text":" "},{"type":"text","text":"location is being tracked."}],"schemaVersion":{"major":0,"patch":0,"minor":3},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}],"abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"provided by Apple’s MapKit."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserTrackingMode/MLNUserTrackingModeFollowWithCourse":{"fragments":[{"text":"MLNUserTrackingModeFollowWithCourse","kind":"identifier"}],"abstract":[],"identifier":"doc:\/\/org.swift.MyProject\/documen